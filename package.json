{
  "name": "user-onboarding-module",
  "main": "src/entry.js",
  "description": "A Node based module using Mongodb to onboard user's into a very basic application, secured using JWT authorization",
  "version": "1.0.0",
  "private": false,
  "engines": {
    "node": "12.*"
  },
  "scripts": {
    "start": "npx babel-node src/entry.js",
    "startWithNodemon": "nodemon --exec babel-node src/entry.js",
    "build": "npx babel src --out-dir build --ignore node_modules",
    "deployment": "node build/entry.js"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "@hapi/hapi": "^20.1.2",
    "@hapi/inert": "^6.0.3",
    "@hapi/vision": "^6.0.1",
    "async": "^3.2.0",
    "babel-node": "0.0.1-security",
    "body-parser": "1.19.0",
    "cfenv": "1.2.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fluent-ffmpeg": "2.1.2",
    "fs": "0.0.1-security",
    "fs-extra": "9.1.0",
    "gm": "1.23.1",
    "handlebars": "^4.7.7",
    "hapi-auth-bearer-token": "8.0.0",
    "hapi-swagger": "^14.1.0",
    "ibm-cos-sdk": "1.10.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "8.5.1",
    "knox": "^0.9.2",
    "lodash": "^4.17.21",
    "log4js": "6.3.0",
    "md5": "2.3.0",
    "moment": "2.29.1",
    "mongoose": "^5.12.2",
    "path": "0.12.7",
    "randomstring": "1.1.5",
    "socket.io": "^4.0.0",
    "underscore": "1.12.1",
    "validator": "13.5.2"
  },
  "author": "Sanchit Dang",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.12",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/preset-env": "^7.13.12"
  }
